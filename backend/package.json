{
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc && cp src/swagger.json dist/",
    "build:stage": "NODE_ENV=stage tsc && cp src/swagger.json dist/",
    "build:production": "NODE_ENV=production tsc && cp src/swagger.json dist/",
    "build:render": "NODE_ENV=production npm install --production=false && npx tsc && cp src/swagger.json dist/",
    "start:stage": "npm run build:stage && NODE_ENV=stage node -r dotenv/config dist/server.js dotenv_config_path=.env.stage",
    "startdev:stage": "NODE_ENV=stage nodemon --exec tsx src/server.ts dotenv_config_path=.env.stage",
    "start:production": "NODE_ENV=production node -r dotenv/config dist/server.js dotenv_config_path=.env.production",
    "dev:stage": "NODE_ENV=stage nodemon --exec tsx src/server.ts dotenv_config_path=.env.stage"
  },
